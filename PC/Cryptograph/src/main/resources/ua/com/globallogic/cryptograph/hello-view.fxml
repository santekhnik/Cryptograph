<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Button?>
<AnchorPane prefHeight="479.0" prefWidth="657.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="ua.com.globallogic.cryptograph.controller.ControllerApplication">
    <Label fx:id="statusLabel" layoutX="255.0" layoutY="435.0" text="Status: Not connected"/>
    <Button fx:id="disconnectButton" disable="true" layoutX="132.0" layoutY="431.0" prefHeight="25.0" prefWidth="101.0"
            text="Disconnect"/>
    <ChoiceBox fx:id="portChoiceBox" layoutX="40.0" layoutY="31.0" prefHeight="25.0" prefWidth="92.0"/>
    <Button fx:id="connectButton" layoutX="21.0" layoutY="431.0" prefHeight="25.0" prefWidth="101.0" text="Connect"/>
    <Button fx:id="chooseFileButton" layoutX="498.0" layoutY="31.0" onAction="#chooseFile" prefHeight="25.0"
            prefWidth="120.0" text="Choose File"/>
    <Label fx:id="selectedFileLabel" layoutX="40.0" layoutY="160.0" prefHeight="17.0" prefWidth="600.0"
           wrapText="true"/>
</AnchorPane>
